classDiagram
    class Order {
        -int number
        -int deliveryTime
        -State state
        +Order(int number, int deliveryTime)
        +getNumber() int
        +getDeliveryTime() int
        +getState() String
        +printState()
        +updateState() boolean
    }
    class State {
        <<enumeration>>
        +State PENDING $
        +State COOKING $
        +State COOKED $
        +State DELIVERING $
        +State COMPLETED $
    }